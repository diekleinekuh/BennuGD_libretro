set(ZIG_TARGET arm-linux-gnueabi.2.12 )
set(CMAKE_SYSTEM_NAME Linux)
set(CMAKE_SYSTEM_PROCESSOR arm)

include( "${CMAKE_CURRENT_LIST_DIR}/zig.toolchain.cmake" )
set(CMAKE_C_FLAGS_INIT "${CMAKE_C_FLAGS_INIT} -mcpu=cortex_a9 -mtune=cortex_a53 -mfloat-abi=softfp" )
set(WITH_NEON false)
set(WITH_RUNTIME_CPU_DETECTION false)
